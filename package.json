{
  "name": "@asd14/m",
  "version": "0.20.0",
  "description": "Point free style, functional Javascript library",
  "homepage": "https://github.com/asd14/m",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asd14/m.git"
  },
  "bugs": {
    "url": "https://github.com/asd14/m/issues"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidcm"
  },
  "main": "dist/index.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "----UTIL-------------------------------------": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "update": "npm-check -u",
    "----BUNDLE-----------------------------------": "",
    "build:docs": "---TBD---",
    "build": "rm -rf ./dist && babel src --out-dir dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm audit fix && npm test && npm run lint && npm run coverage",
    "----TEST-------------------------------------": "",
    "lint": "eslint src",
    "bench": "node src/core__deep-equal/deep-equal.bench.js",
    "test": "tape -r @babel/register src/*.test.js src/**/*.test.js",
    "test-ui": "tape-ui -r @babel/register src/*.test.js src/**/*.test.js",
    "tdd": "nodemon --watch src --exec \"npm test | tap-diff\"",
    "coverage:report": "NODE_ENV=test nyc npm test && nyc report --reporter=html",
    "coverage": "NODE_ENV=test nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "point-free",
    "functional",
    "javascript",
    "library"
  ],
  "devDependencies": {
    "@asd14/eslint-config": "4.0.2",
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-pipeline-operator": "7.3.2",
    "@babel/preset-env": "7.4.3",
    "@babel/register": "7.4.0",
    "babel-eslint": "10.0.1",
    "benchmark": "2.1.4",
    "chi-squared-test": "1.1.0",
    "coveralls": "3.0.3",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.17.1",
    "eslint-plugin-json": "1.4.0",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-unicorn": "8.0.2",
    "fast-deep-equal": "2.0.1",
    "lodash": "4.17.11",
    "markdownlint-cli": "0.15.0",
    "nodemon": "1.18.11",
    "npm-check": "5.9.0",
    "nyc": "13.3.0",
    "prettier": "1.17.0",
    "pretty-error": "2.1.1",
    "ramda": "0.26.1",
    "tap-diff": "0.1.1",
    "tape": "4.10.1",
    "underscore": "1.9.1"
  }
}
