{
  "name": "@asd14/m",
  "version": "0.23.0",
  "description": "Point free style, functional Javascript library",
  "homepage": "https://github.com/asd14/m",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asd14/m.git"
  },
  "bugs": {
    "url": "https://github.com/asd14/m/issues"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidcm"
  },
  "main": "dist/index.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "----UTIL": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "setup:ci": "npm ci && npm audit fix",
    "update": "npm-check --update --save-exact",
    "----BUNDLE": "",
    "build:docs": "---TBD---",
    "build": "rm -rf ./dist && babel src --out-dir dist",
    "build:ci": "babel src --out-dir dist",
    "----TEST": "",
    "tdd": "nodemon --watch src --exec \"npm test\"",
    "test": "tape -r @babel/register src/*.test.js src/**/*.test.js | tap-nirvana",
    "test:ci": "tape dist/*.test.js dist/**/*.test.js | tap-nirvana",
    "test-ui": "tape-ui -r @babel/register src/*.test.js src/**/*.test.js",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "eslint src",
    "lint:md": "markdownlint *.md",
    "coverage:report": "nyc npm test && nyc report --reporter=html",
    "coverage:ci": "nyc npm run test && nyc report --reporter=text-lcov | coveralls",
    "----OTHER": "",
    "bench": "node src/core__deep-equal/deep-equal.bench.js"
  },
  "keywords": [
    "point-free",
    "functional",
    "javascript",
    "library"
  ],
  "devDependencies": {
    "@asd14/eslint-config": "5.2.0",
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/plugin-proposal-pipeline-operator": "7.5.0",
    "@babel/preset-env": "7.6.0",
    "@babel/register": "7.6.0",
    "babel-eslint": "10.0.3",
    "benchmark": "2.1.4",
    "chi-squared-test": "1.1.0",
    "coveralls": "3.0.6",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-json": "1.4.0",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-unicorn": "10.0.0",
    "fast-deep-equal": "2.0.1",
    "lodash": "4.17.15",
    "markdownlint-cli": "0.18.0",
    "nodemon": "1.19.2",
    "npm-check": "5.9.0",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "pretty-error": "2.1.1",
    "ramda": "0.26.1",
    "semantic-release": "15.13.24",
    "tap-nirvana": "^1.1.0",
    "tape": "4.11.0",
    "underscore": "1.9.1"
  }
}
