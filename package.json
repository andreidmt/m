{
  "name": "@codemachiner/m",
  "version": "0.13.2",
  "description": "Functional library for Javascript",
  "homepage": "https://github.com/codemachiner/m",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codemachiner/m.git"
  },
  "bugs": {
    "url": "https://github.com/codemachiner/m/issues"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "email": "andrei@codemachiner.com"
  },
  "main": "dist/index.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "----UTIL-------------------------------------": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "update": "npm-check -u",
    "----BUNDLE-----------------------------------": "",
    "build:docs": "---TBD---",
    "build": "rm -rf ./dist && babel src --out-dir dist",
    "prepare": "npm audit fix && npm test && npm run lint && npm run build",
    "prepublishOnly": "npm run coverage",
    "----TEST-------------------------------------": "",
    "lint": "eslint src",
    "bench": "node src/core__deep-equal/deep-equal.bench.js",
    "test": "tape -r @babel/register src/*.test.js src/**/*.test.js",
    "tdd": "nodemon --watch src --exec \"npm run test | tap-diff\"",
    "coverage:report": "NODE_ENV=test nyc npm test && nyc report --reporter=html",
    "coverage": "NODE_ENV=test nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "functional",
    "library",
    "fp",
    "pure"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-pipeline-operator": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/register": "7.0.0",
    "@codemachiner/eslint-config": "3.0.0",
    "babel-eslint": "10.0.1",
    "benchmark": "2.1.4",
    "chi-squared-test": "1.1.0",
    "coveralls": "3.0.2",
    "eslint": "5.8.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-json": "1.2.1",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-unicorn": "6.0.1",
    "fast-deep-equal": "2.0.1",
    "lodash": "4.17.11",
    "markdownlint-cli": "0.13.0",
    "nodemon": "1.18.6",
    "npm-check": "5.9.0",
    "nyc": "13.1.0",
    "prettier": "1.14.3",
    "ramda": "0.25.0",
    "tap-diff": "0.1.1",
    "tape": "4.9.1",
    "underscore": "1.9.1"
  }
}
