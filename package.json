{
  "name": "@codemachiner/m",
  "version": "0.4.0",
  "description": "Functional library for Javascript",
  "homepage": "https://github.com/codemachiner/m",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codemachiner/m.git"
  },
  "bugs": {
    "url": "https://github.com/codemachiner/m/issues"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "email": "andrei@codemachiner.com"
  },
  "main": "dist/index.js",
  "scripts": {
    "----UTIL-------------------------------------": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "update": "npm-check -u",
    "----BUNDLE-----------------------------------": "",
    "build:docs": "---TBD---",
    "build": "babel src --out-dir dist",
    "prepublish": "npm test && npm run lint && npm run build",
    "----TEST-------------------------------------": "",
    "flow": "flow check",
    "lint": "eslint --fix ./src",
    "bench": "node src/core__deep-equal/deep-equal.bench.js",
    "test": "tape -r @babel/register src/**/*.test.js | tap-diff",
    "tdd": "nodemon --watch src --exec \"npm run test\""
  },
  "keywords": [
    "functional",
    "library",
    "fp",
    "pure"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/register": "7.0.0",
    "@codemachiner/eslint-config": "2.9.7",
    "babel-eslint": "9.0.0",
    "benchmark": "2.1.4",
    "eslint": "5.4.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-json": "1.2.1",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-promise": "4.0.0",
    "eslint-plugin-unicorn": "6.0.0",
    "fast-deep-equal": "2.0.1",
    "flow-bin": "0.81.0",
    "lodash": "4.17.10",
    "markdownlint-cli": "0.13.0",
    "nodemon": "1.18.4",
    "npm-check": "5.8.0",
    "ramda": "0.25.0",
    "tap-diff": "0.1.1",
    "tape": "4.9.1",
    "underscore": "1.9.1"
  },
  "license": "MIT"
}
