{
  "name": "@asd14/m",
  "version": "0.0.1",
  "description": "Point free style, functional Javascript library",
  "license": "MIT",
  "homepage": "https://github.com/asd-xiv/m",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asd-xiv/m.git"
  },
  "bugs": {
    "url": "https://github.com/asd-xiv/m/issues"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidmt"
  },
  "keywords": [
    "point-free",
    "functional",
    "javascript",
    "library"
  ],
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "----UTIL": "",
    "prepare": "git config --local core.hooksPath .githook",
    "presetup": "rm -rf ./node_modules",
    "setup": "npm ci --also=dev",
    "update": "npm-check --update",
    "postupdate": "npm audit fix",
    "----BUNDLE": "",
    "prebuild": "rm -rf ./dist",
    "build": "babel src --out-dir dist",
    "build:docs": "documentation build src/index.js --project-name m -f html -o docs -c documentation.yml",
    "----TEST": "",
    "pretest": "npm run build",
    "test": "tape 'dist/*.test.js' 'dist/**/*.test.js' | tap-nirvana",
    "test-ui": "tape-ui 'dist/**/*.test.js' 'dist/**/*.test.jsx'",
    "tdd": "nodemon --watch src --exec 'npm test'",
    "coverage": "nyc npm run test && nyc report --reporter=text-lcov | coveralls",
    "----LINT": "",
    "lint:js": "eslint --quiet src",
    "lint:js-staged": "eslint --quiet",
    "lint:md": "markdownlint '*.md'",
    "lint:md-staged": "markdownlint",
    "lint": "npm run lint:js && npm run lint:md",
    "----PUBLISH": "",
    "publish": "semantic-release"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.13.10",
    "core-js": "^3.9.1",
    "fast-deep-equal": "^3.1.3",
    "rfc-3986": "^1.0.1"
  },
  "devDependencies": {
    "@asd14/eslint-config": "^5.31.0",
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.13",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/register": "^7.13.8",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "benchmark": "^2.1.4",
    "chi-squared-test": "^1.1.0",
    "coveralls": "^3.1.0",
    "documentation": "^13.2.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-no-inferred-method-name": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-unicorn": "^29.0.0",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "markdownlint-cli": "^0.27.1",
    "nodemon": "^2.0.7",
    "npm-check": "^5.9.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "pretty-error": "^3.0.3",
    "ramda": "^0.27.1",
    "riteway": "^6.2.1",
    "semantic-release": "^17.4.2",
    "tap-nirvana": "^1.1.0",
    "tape": "^5.2.2",
    "underscore": "^1.12.1"
  }
}
